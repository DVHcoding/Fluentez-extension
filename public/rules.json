[
    {
        "id": 1,
        "priority": 1,
        "action": {
            "type": "modifyHeaders",
            "responseHeaders": [
                {
                    "header": "access-control-allow-origin",
                    "operation": "set",
                    "value": "*"
                },
                {
                    "header": "access-control-allow-methods",
                    "operation": "set",
                    "value": "get, post, put, delete, options"
                },
                {
                    "header": "access-control-allow-headers",
                    "operation": "set",
                    "value": "*"
                }
            ],
            "requestHeaders": [
                {
                    "header": "origin",
                    "operation": "set",
                    "value": "https://fluentez.com"
                },
                {
                    "header": "referer",
                    "operation": "set",
                    "value": "https://fluentez.com/"
                }
            ]
        },
        "condition": {
            "urlFilter": "https://api.fluentez.com/*",
            "responseTypes": ["xmlhttprequest"],
            "excludedDomains": ["fluentez.com"]
        }
    }
]
